{
  "schemaVersion": 1,
  "id"           : "storagenetworks",
  "version"      : "${version}",
  "name"         : "Storage Networks",
  "description"  : "Clone of Storage Networks for Fabric Loader, for lazy people who doesn't want to sort chests!",
  "authors"      : ["deirn"],
  "contributors" : [],
  "contact"      : {
    "homepage": "https://gitlab.com/intended/fabric/fsn",
    "sources" : "https://gitlab.com/intended/fabric/fsn",
    "issues"  : "https://gitlab.com/intended/fabric/fsn/-/issues"
  },
  "license"      : "MIT",
