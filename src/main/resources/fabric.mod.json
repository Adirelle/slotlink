  "environment"  : "*",
  "entrypoints"  : {
    "main"   : [{"adapter": "kotlin", "value": "io.gitlab.intended.storagenetworks.Mod::main"}],
    "client" : [{"adapter": "kotlin", "value": "io.gitlab.intended.storagenetworks.Mod::client"}],
    "modmenu": [{"adapter": "kotlin", "value": "io.gitlab.intended.storagenetworks.ModMenu"}]
