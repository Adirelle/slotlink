  "authors"      : ["deirn"],
  "contributors" : [],
  "contact"      : {
    "homepage": "https://github.com/badasintended/slotlink",
    "sources" : "https://github.com/badasintended/slotlink",
    "issues"  : "https://github.com/badasintended/slotlink/issues"
  },
  "license"      : "LGPL-3.0",
