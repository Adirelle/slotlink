{
  "multipart": [
    {
      "apply": {
        "model": "storagenetworks:block/cable_center"
      }
    },
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "storagenetworks:block/cable_side"
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "storagenetworks:block/cable_side",
        "y": 180
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "storagenetworks:block/cable_side",
        "y": 90
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "storagenetworks:block/cable_side",
        "y": -90
      }
    },
    {
      "when": {
        "up": true
      },
      "apply": {
        "model": "storagenetworks:block/cable_side",
        "x": -90
      }
    },
    {
      "when": {
        "down": true
      },
      "apply": {
        "model": "storagenetworks:block/cable_side",
        "x": 90
      }
    }
  ]
}